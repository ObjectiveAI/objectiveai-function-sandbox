{
  "name": "objectiveai-function",
  "version": "1.0.0",
  "main": "main.ts",
  "scripts": {
    "init": "node preInit.js && ts-node init.ts",
    "build": "ts-node test.ts && ts-node build.ts",
    "build-final": "git -C objectiveai checkout . && npm run build",
    "install-rust-logs": "npm run install",
    "publish": "npm run build && ts-node publish.ts",
    "commit-and-push": "ts-node commitAndPush.ts",
    "update-sandbox": "ts-node updateSandbox.ts && npm run commit-and-push -- -m \"update sandbox\" --skip-checkout"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "objectiveai": "file:./objectiveai/objectiveai-js",
    "zod": "^4.3.5"
  }
}
